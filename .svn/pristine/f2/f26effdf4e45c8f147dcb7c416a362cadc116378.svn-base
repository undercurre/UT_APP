import request from '@/utils/request.js'
import { requestOptions } from '@/utils/utils.js'

export function TUOperationSave(data) {
	return request.post('api/TUOperation/TUOperationSave', data, requestOptions())
}

export function LoadDataEX(query) {
	return request.get('api/SfcsWo/LoadDataEX', requestOptions(query))
}
